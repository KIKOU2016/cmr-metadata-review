#!/bin/bash -x
# To Copy and rename the yml files in the config directory:-
#   * Type './configure
mkdir -p tmp/pids
mkdir shared
mkdir shared/pids
mkdir shared/log
mkdir shared/sockets

cp config/sidekiq.example.yml config/sidekiq.yml
cp config/redis.example.yml config/redis.yml
cp config/schedule.example.yml config/schedule.yml